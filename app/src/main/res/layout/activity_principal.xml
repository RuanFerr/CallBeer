<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Principal">

    <TextView
        android:id="@+id/textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="Principal"
        android:textSize="30dp"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="0dp"
        android:layout_height="200dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:srcCompat="@drawable/common_google_signin_btn_text_light" />

    <Button
        android:id="@+id/btCervejas"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="Cervejas"
        android:background="@drawable/rounded_button"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/btChopes"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="10dp"
        android:background="@drawable/rounded_button"
        android:text="chopes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <Button
        android:id="@+id/btEnergeticos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_button"
        android:text="Energéticos"
        app:layout_constraintEnd_toEndOf="@+id/btCervejas"
        app:layout_constraintStart_toStartOf="@+id/btCervejas"
        app:layout_constraintTop_toBottomOf="@+id/btCervejas" />

    <Button
        android:id="@+id/btMista"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_button"
        android:text="bebidas mistas"
        app:layout_constraintEnd_toEndOf="@+id/btChopes"
        app:layout_constraintStart_toStartOf="@+id/btChopes"
        app:layout_constraintTop_toBottomOf="@+id/btChopes" />

    <Button
        android:id="@+id/btRefrigerantes"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_button"
        android:text="refrigerantes"
        app:layout_constraintEnd_toEndOf="@+id/btEnergeticos"
        app:layout_constraintStart_toStartOf="@+id/btEnergeticos"
        app:layout_constraintTop_toBottomOf="@+id/btEnergeticos" />

    <Button
        android:id="@+id/btSucos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_button"
        android:text="sucos"
        app:layout_constraintEnd_toEndOf="@+id/btMista"
        app:layout_constraintStart_toStartOf="@+id/btMista"
        app:layout_constraintTop_toBottomOf="@+id/btMista" />

    <Button
        android:id="@+id/btIsotonicos"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_button"
        android:text="Isotônicos"
        app:layout_constraintEnd_toEndOf="@+id/btRefrigerantes"
        app:layout_constraintStart_toStartOf="@+id/btRefrigerantes"
        app:layout_constraintTop_toBottomOf="@+id/btRefrigerantes" />

    <Button
        android:id="@+id/btAguasEChas"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/rounded_button"
        android:text="águas e chás"
        app:layout_constraintEnd_toEndOf="@+id/btSucos"
        app:layout_constraintStart_toStartOf="@+id/btSucos"
        app:layout_constraintTop_toBottomOf="@+id/btSucos" />

    <Button
        android:id="@+id/btnRec"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/rounded_button"
        android:text="teste Reclamacao"
        app:layout_constraintEnd_toEndOf="@+id/btAguasEChas"
        app:layout_constraintStart_toStartOf="@+id/btIsotonicos"
        app:layout_constraintTop_toBottomOf="@+id/btAguasEChas" />
</androidx.constraintlayout.widget.ConstraintLayout>