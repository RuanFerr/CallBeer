<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.multistar.callbeer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ConfirmacaoPedido"></activity>
        <activity android:name=".Central">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Pedidos" />
        <activity android:name=".Carrinho" />
        <activity android:name=".Reclamacao" />
        <activity android:name=".Pedido">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Principal" />
        </activity>
        <activity android:name=".Principal"></activity>
        <activity android:name=".CadastroPessoa" />
        <activity android:name=".CadastroEmpresa" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>